<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tistory.qlyh8.emarket">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key"/>

        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".info.InfoActivity"/>
        <activity android:name=".LoginActivity"/>
        <activity android:name=".ProfileActivity"/>
        <activity android:name=".usePattern.UsePatternViewActivity"/>
        <activity android:name=".dataManager.UseMonthExcelFile"/>
        <activity android:name=".dataManager.LoadExcelFiles"/>
        <activity android:name=".initialize.InitialPowerNumberActivity"/>
        <activity android:name=".initialize.InitialTypeActivity"/>
        <activity android:name=".trade.TradeMainActivity"/>
        <activity android:name=".trade.TradeStatusActivity"/>
        <activity android:name=".report.ReportViewActivity"/>
    </application>

</manifest>